alias fr='setxkbmap fr'
alias us='setxkbmap us'
alias maj='sudo pacman -Syu'
alias cleancache='sudo pacman -Scc'
alias lrt='ls -lrt'
alias ll='ls -al'
alias dirs='dirs -v'
